class Solution(object):
    def findWords(self, words):
        """
        :type words: List[str]
        :rtype: List[str]
        """
        new=[]

        row1 = set("qwertyuiop")
        row2 = set("asdfghjkl")
        row3 = set("zxcvbnm")

        for word in words:
            w=set(word.lower())
            if w.issubset(row1) or w.issubset(row2) or w.issubset(row3):
                new.append(word)
        
        return new
             
        
