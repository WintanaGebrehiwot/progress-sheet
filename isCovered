class Solution(object):
    def isCovered(self, ranges, left, right):
        """
        :type ranges: List[List[int]]
        :type left: int
        :type right: int
        :rtype: bool
        """        
        for num in range(left, right + 1):
            
            covered = False
            
            for r in ranges:
                if r[0] <= num <= r[1]:
                    covered = True
                    break
            
            if not covered:
                return False
        
        return True
