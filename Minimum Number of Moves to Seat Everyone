class Solution(object):
    def minMovesToSeat(self, seats, students):
        """
        :type seats: List[int]
        :type students: List[int]
        :rtype: int
        """
        seats.sort()
        students.sort()
        moves = 0
        for s, st in zip(seats, students):
            moves += abs(s - st)
        return moves
        

        

        
        
