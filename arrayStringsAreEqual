class Solution(object):
    def arrayStringsAreEqual(self, word1, word2):
        """
        :type word1: List[str]
        :type word2: List[str]
        :rtype: bool
        """
        word1new=""
        word2new=""
        for i in word1:
            word1new+=i
        for i in word2:
            word2new+=i
        if word1new==word2new:
            return (True)
        else:
            return (False)
