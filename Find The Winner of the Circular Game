class Solution(object):
    def findTheWinner(self, n, k):
        """
        :type n: int
        :type k: int
        :rtype: int
        """
        arr=[]
        for i in range(n):
            arr.append(i+1)

        index = 0
        while len(arr) > 1:
            index = (index + k - 1) % len(arr)
            arr.pop(index)
        return arr[0]
            

        
