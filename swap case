def swap_case(s):
    result=[]
    for i in s:
        if i==i.lower():
            result.append(i.upper())
        elif i==i.upper():
            result.append(i.lower())
        else: result.append(i)
            
    return ''.join(result)
    
if __name__ == '__main__':
    s = input()
    result = swap_case(s)
    print(result)
